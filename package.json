{
  "name": "secure-rm",
  "version": "1.1.1",
  "description": "Completely erases files by making recovery impossible.",
  "main": "index.js",
  "scripts": {
    "start": "node bin/run",
    "release": "np",
    "style": "standard --verbose --env jest",
    "dev-setup": "npm install -g standard jest np",
    "test": "jest",
    "readme": "node tools/readme-helper"
  },
  "dependencies": {
    "@oclif/command": "^1.5.18",
    "@oclif/parser": "^3.8.4",
    "chalk": "^2.4.2",
    "glob": "^7.1.4",
    "inquirer": "^6.5.0",
    "ololog": "^1.1.146",
    "rimraf": "^2.6.3",
    "tty-table": "^2.7.3"
  },
  "devDependencies": {
    "jest": "^24.8.0",
    "np": "^5.0.3",
    "standard": "^12.0.1"
  },
  "author": "Oganexon @oganexon",
  "bugs": "https://github.com/oganexon/secure-rm/issues",
  "homepage": "https://github.com/oganexon/secure-rm#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oganexon/secure-rm.git"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "oclif": {
    "bin": "secure-rm"
  },
  "np": {
    "yarn": false
  },
  "bin": {
    "secure-rm": "./bin/run"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "keywords": [
    "privacy",
    "secure",
    "data erasure",
    "file deletion",
    "remove",
    "delete",
    "wipe out",
    "clean",
    "erase",
    "shred",
    "overwrite",
    "glob"
  ],
  "license": "MIT"
}
